{% extends 'base.html.twig' %}

{% block content %}
    <div class="row mt-5">
        <h3 class="text-center text-uppercase">Inscription</h3>
    </div>
    <div class="row mb-4">
        <div class="col-6 mx-auto">
            {{ form_start(form, {'attr':{novalidate:'novalidate'}})}} {# <form action='' metho='' enctype='multipart'> #}
                 {{ form_row(form.firstname)}}
                 {{ form_row(form.lastname)}}
                 {{ form_row(form.email)}}
                 {{ form_row(form.password)}}
            {{ form_end(form)}}{# </form> #}
        </div>
    </div>

{% endblock %}
